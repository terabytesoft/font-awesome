{
    "name": "yii-extension/fontawesome",
    "type": "library",
    "description": "Assets Font Awesome Free",
    "keywords": [
        "yii3",
        "assets",
        "font awesome",
        "free"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "fortawesome/font-awesome": "^5.15.3",
        "mnsami/composer-custom-directory-installer": "*",
        "yiisoft/assets": "^1.0@dev"
    },
    "require-dev": {
        "psr/log": "^1.1",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.4",
        "yiisoft/aliases": "^2.0.0",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/files": "^1.0.0"
    },
    "autoload": {
        "psr-4": {"Yii\\Extension\\Fontawesome\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Yii\\Extension\\Fontawesome\\Tests\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        },
        "installer-paths": {
            "./node_modules/{$vendor}/{$name}": ["fortawesome/font-awesome"]
        }
    },
    "config": {
        "sort-packages": true
    }
}
