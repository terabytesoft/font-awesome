{
    "name": "yii-extension/fontawesome",
    "type": "library",
    "description": "Assets Font Awesome Free",
    "keywords": [
        "yii3",
        "assets",
        "font awesome",
        "free"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "yiisoft/assets": "@dev"
    },
    "require-dev": {
        "psr/log": "^1.1",
        "foxy/foxy": "@dev",
        "phpunit/phpunit": "^9.0",
        "yiisoft/aliases": "@dev",
        "yiisoft/composer-config-plugin": "^1.0@dev",
        "yiisoft/di": "@dev",
        "yiisoft/files": "@dev"
    },
    "autoload": {
        "psr-4": {"Yii\\Extension\\Fontawesome\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Yii\\Extension\\Fontawesome\\Tests\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        },
        "config-plugin": {
            "tests": "config/test.php"
        }
    },
    "config": {
        "sort-packages": true
    }
}
